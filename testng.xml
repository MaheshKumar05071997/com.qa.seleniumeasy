<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
	<listeners>
		<listener
			class-name="com.qa.seleniumeasy.listeners.ListenerClass" />
	</listeners>
	<parameter name="browserName" value="chrome"></parameter>
	<test name="ValidateSimple">
		<classes>
			<class name="com.qa.seleniumeasy.tests.ValidateSimple" />
		</classes>
	</test>

	<test name="ValidatePaginations">
		<classes>
			<class name="com.qa.seleniumeasy.tests.ValidatePaginations" />
		</classes>
	</test>

	<test name="ValidateDataFetch">
		<classes>
			<class name="com.qa.seleniumeasy.tests.ValidateDataFetch" />
		</classes>
	</test>

	<test name="ValidateDragAndDrop">
		<classes>
			<class name="com.qa.seleniumeasy.tests.ValidateDragAndDrop" />
		</classes>
	</test>
	<test name="ValidateEnteringValuesWithExcel">
		<classes>
			<class
				name="com.qa.seleniumeasy.tests.ValidateEnteringValueswithExcel" />
		</classes>
	</test>
	<test name="ValidateDate">
		<classes>
			<class name="com.qa.seleniumeasy.tests.ValidateDate" />
		</classes>
	</test>
</suite>

